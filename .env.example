# EAM Cognitive OS - Environment Variables
# Copy to .env and fill in your values

# ─────────────────────────────────────────────────────────────────────────────
# Vercel AI Gateway (OpenAI-compatible endpoint)
# ─────────────────────────────────────────────────────────────────────────────
VERCEL_AI_GATEWAY_URL=https://ai-gateway.vercel.sh/v1
VERCEL_AI_GATEWAY_TOKEN=your_vercel_ai_gateway_token_here

# Default models (format: provider/model-name)
DEFAULT_MODEL=openai/gpt-4o-mini
EMBEDDING_MODEL=openai/text-embedding-3-small

# ─────────────────────────────────────────────────────────────────────────────
# Supabase Configuration
# ─────────────────────────────────────────────────────────────────────────────
SUPABASE_URL=https://hqntrvhdvfosutewyvei.supabase.co
SUPABASE_ANON_KEY=your_anon_key_here
SUPABASE_SERVICE_ROLE_KEY=your_service_role_key_here

# ─────────────────────────────────────────────────────────────────────────────
# Application Settings
# ─────────────────────────────────────────────────────────────────────────────
APP_NAME=EAM Cognitive OS
APP_VERSION=2.0.1
DEBUG=false

# CORS (comma-separated list of origins)
CORS_ORIGINS=["http://localhost:3000","http://localhost:5173"]

# ─────────────────────────────────────────────────────────────────────────────
# Security
# ─────────────────────────────────────────────────────────────────────────────
HITL_TIMEOUT_HOURS=24
MAX_AGENT_ITERATIONS=10
